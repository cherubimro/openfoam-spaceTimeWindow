#!/bin/sh
cd "${0%/*}" || exit                                # Run from this directory
. ${WM_PROJECT_DIR:?}/bin/tools/CleanFunctions      # Tutorial clean functions
#------------------------------------------------------------------------------

echo "Cleaning spaceTimeWindowInitCase utility..."
wclean src/spaceTimeWindow/applications/utilities/preProcessing/spaceTimeWindowInitCase

echo "Cleaning spaceTimeWindow library..."
wclean src/spaceTimeWindow

# Remove binaries from user directories
echo "Removing installed binaries..."
rm -f "$FOAM_USER_LIBBIN/libspaceTimeWindow.so" 2>/dev/null
rm -f "$FOAM_USER_APPBIN/spaceTimeWindowInitCase" 2>/dev/null

echo ""
echo "Clean complete."

#------------------------------------------------------------------------------
